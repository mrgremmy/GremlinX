{
    "name": "@btc-vision/bitcoin",
    "type": "module",
    "version": "7.0.0-rc.6",
    "sideEffects": false,
    "description": "Client-side Bitcoin JavaScript library",
    "engines": {
        "node": ">=24.0.0"
    },
    "exports": {
        ".": {
            "react-native": {
                "types": "./build/index.d.ts",
                "import": "./build/index.js",
                "default": "./build/index.js"
            },
            "browser": {
                "types": "./browser/index.d.ts",
                "import": "./browser/index.js",
                "default": "./browser/index.js"
            },
            "node": {
                "types": "./build/index.d.ts",
                "import": "./build/index.js",
                "default": "./build/index.js"
            },
            "types": "./build/index.d.ts",
            "import": "./build/index.js",
            "default": "./build/index.js"
        },
        "./browser": {
            "types": "./browser/index.d.ts",
            "import": "./browser/index.js",
            "default": "./browser/index.js"
        },
        "./workers": {
            "react-native": {
                "types": "./build/workers/index.react-native.d.ts",
                "import": "./build/workers/index.react-native.js",
                "default": "./build/workers/index.react-native.js"
            },
            "browser": {
                "types": "./browser/workers/index.d.ts",
                "import": "./browser/workers/index.js",
                "default": "./browser/workers/index.js"
            },
            "node": {
                "types": "./build/workers/index.node.d.ts",
                "import": "./build/workers/index.node.js",
                "default": "./build/workers/index.node.js"
            },
            "types": "./build/workers/index.d.ts",
            "import": "./build/workers/index.js",
            "default": "./build/workers/index.js"
        },
        "./address": {
            "types": "./build/address.d.ts",
            "import": "./build/address.js"
        },
        "./script": {
            "types": "./build/script.d.ts",
            "import": "./build/script.js"
        },
        "./crypto": {
            "types": "./build/crypto.d.ts",
            "import": "./build/crypto.js"
        },
        "./transaction": {
            "types": "./build/transaction.d.ts",
            "import": "./build/transaction.js"
        },
        "./block": {
            "types": "./build/block.d.ts",
            "import": "./build/block.js"
        },
        "./psbt": {
            "types": "./build/psbt.d.ts",
            "import": "./build/psbt.js"
        },
        "./networks": {
            "types": "./build/networks.d.ts",
            "import": "./build/networks.js"
        },
        "./payments": {
            "types": "./build/payments/index.d.ts",
            "import": "./build/payments/index.js"
        },
        "./payments/*": {
            "types": "./build/payments/*.d.ts",
            "import": "./build/payments/*.js"
        },
        "./io": {
            "types": "./build/io/index.d.ts",
            "import": "./build/io/index.js"
        },
        "./ecc": {
            "types": "./build/ecc/index.d.ts",
            "import": "./build/ecc/index.js"
        },
        "./types": {
            "types": "./build/types.d.ts",
            "import": "./build/types.js"
        },
        "./errors": {
            "types": "./build/errors.d.ts",
            "import": "./build/errors.js"
        }
    },
    "browser": {
        "./build/index.js": "./browser/index.js",
        "./build/index.d.ts": "./browser/index.d.ts",
        "./build/workers/index.node.js": "./browser/workers/index.js",
        "./build/workers/index.node.d.ts": "./browser/workers/index.d.ts",
        "Buffer": "buffer",
        "crypto": "./src/crypto/crypto-browser.js",
        "stream": "stream-browserify",
        "zlib": "browserify-zlib"
    },
    "react-native": {
        "./build/crypto-hashes.js": "./build/crypto-hashes.native.js"
    },
    "keywords": [
        "bitcoinjs",
        "bitcoin",
        "browserify",
        "javascript",
        "bitcoinjs"
    ],
    "license": "MIT",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "typings": "build/index.d.ts",
    "module": "build/index.js",
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/btc-vision/bitcoin.git"
    },
    "scripts": {
        "watch": "tsc --watch",
        "build": "eslint src && tsc --build tsconfig.build.json",
        "setup": "npm i && npm run build",
        "browserBuild": "vite build --config vite.config.browser.ts",
        "audit": "better-npm-audit audit -l high",
        "clean": "rimraf build",
        "coverage": "vitest run --coverage",
        "doc": "typedoc --options typedoc.json",
        "format:ci": "npm run prettier -- --check && npm run prettier -- --check",
        "gitdiff:ci": "npm run build && git diff --exit-code",
        "integration": "vitest run --config vitest.config.integration.ts",
        "lint": "eslint src/**",
        "lint:tests": "eslint test/**/*.spec.ts",
        "prettier": "prettier \"src/**/*.ts\" \"test/**/*.ts\" --ignore-path ./.prettierignore",
        "test": "npm run build && npm run lint && vitest run",
        "test:watch": "vitest",
        "unit": "vitest run",
        "test:browser": "vitest run --config vitest.config.browser.ts",
        "test:browser:watch": "vitest --config vitest.config.browser.ts",
        "bench": "vitest bench benchmark/",
        "bench:run": "npx tsx benchmark/signing.bench.ts",
        "check:circular": "madge --circular --extensions ts,tsx src/",
        "prebuild:ecc": "npx tsx scripts/bundle-ecc.ts",
        "prebuild": "npm run prebuild:ecc && npm run check:circular"
    },
    "devDependencies": {
        "@btc-vision/bs58check": "^5.0.1",
        "@eslint/js": "^10.0.1",
        "@scure/base": "^2.0.0",
        "@types/node": "^25.2.3",
        "@types/randombytes": "^2.0.3",
        "@vitest/browser": "^4.0.18",
        "@vitest/browser-playwright": "^4.0.18",
        "@vitest/coverage-v8": "^4.0.18",
        "better-npm-audit": "^3.11.0",
        "bip39": "^3.1.0",
        "bip65": "^1.0.3",
        "bip68": "^1.0.4",
        "dhttp": "^3.0.3",
        "eslint": "^10.0.0",
        "https-browserify": "^1.0.0",
        "madge": "^8.0.0",
        "minimaldata": "^1.0.2",
        "os-browserify": "^0.3.0",
        "playwright": "^1.58.2",
        "prettier": "^3.8.1",
        "randombytes": "^2.1.0",
        "regtest-client": "0.2.1",
        "rimraf": "^6.1.3",
        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "tiny-secp256k1": "^2.2.4",
        "ts-node": "^10.9.2",
        "typedoc": "^0.28.17",
        "typedoc-material-theme": "^1.4.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0",
        "vite": "^7.3.1",
        "vite-plugin-dts": "^4.5.4",
        "vite-plugin-node-polyfills": "^0.25.0",
        "vite-plugin-top-level-await": "^1.6.0",
        "vite-plugin-wasm": "^3.5.0",
        "vitest": "^4.0.18"
    },
    "peerDependencies": {
        "react-native-quick-crypto": ">=1.0.0",
        "react-native-worklets": ">=0.7.0"
    },
    "peerDependenciesMeta": {
        "react-native-worklets": {
            "optional": true
        },
        "react-native-quick-crypto": {
            "optional": true
        }
    },
    "dependencies": {
        "@btc-vision/bip32": "^7.1.2",
        "@btc-vision/ecpair": "^4.0.5",
        "@btc-vision/logger": "^1.0.8",
        "@noble/hashes": "^2.0.1",
        "@noble/secp256k1": "^3.0.0",
        "assert": "^2.1.0",
        "bech32": "^2.0.0",
        "bip174": "^3.0.0",
        "browserify-zlib": "^0.2.0",
        "buffer": "^6.0.3",
        "process": "^0.11.10",
        "varuint-bitcoin": "^2.0.0"
    }
}
